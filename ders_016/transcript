# Loading work.fifo
add wave -position insertpoint  \
sim:/tb_fifo/DATA_WIDTH \
sim:/tb_fifo/FIFO_DEPTH \
sim:/tb_fifo/CLK_PERIOD \
sim:/tb_fifo/clk \
sim:/tb_fifo/rst \
sim:/tb_fifo/wr_en \
sim:/tb_fifo/rd_en \
sim:/tb_fifo/wdata \
sim:/tb_fifo/rdata \
sim:/tb_fifo/full \
sim:/tb_fifo/empty
restart
run -all
